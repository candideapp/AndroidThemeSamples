<?xml version="1.0" encoding="utf-8"?>
<resources>
	<style name="Light1">
		<item name="android:background">@drawable/light1_box</item>
		<item name="android:textColor">#666666</item>
		<item name="android:shadowColor">#ffffff</item>
		<item name="android:shadowDy">2</item>
	  	<item name="android:shadowDx">2</item>
	  	<item name="android:shadowRadius">2</item>
	</style>
	<style name="Light1.Text">
		<item name="android:background">#00f2f2f2</item>
		<item name="android:textColor">#ffffff</item>
	  	<item name="android:shadowColor">#666666</item>
	  	<item name="android:shadowDy">1</item>
	  	<item name="android:shadowDx">1</item>
	  	<item name="android:shadowRadius">1</item>
	</style>
	<style name="Light1.Text.Medium">
	  	<item name="android:textSize">16dp</item>
	</style>
	<style name="Light1.Text.Medium.Bold">
		<item name="android:textStyle">bold</item>
	</style>
	<style name="Light1.Text.Dark">
		<item name="android:textColor">#666666</item>
		<item name="android:shadowColor">#ffffff</item>
	</style>
	<style name="Light1.Text.Dark.Medium">
	  	<item name="android:textSize">16dp</item>
	</style>
	<style name="Light1.Text.Dark.Medium.Bold">
		<item name="android:textStyle">bold</item>
	</style>
	
	<style name="Light1.Button">
		<item name="android:textColor">#ffffff</item>
	  	<item name="android:shadowColor">#666666</item>
	  	<item name="android:shadowDy">1</item>
	  	<item name="android:shadowDx">1</item>
	  	<item name="android:shadowRadius">1</item>
	  	<item name="android:background">@drawable/light1_button1</item>
	</style>
	<style name="Light1.Button.Medium">
	  	<item name="android:textSize">16dp</item>
	</style>
	<style name="Light1.Button.Medium.Bold">
		<item name="android:textStyle">bold</item>
	</style>
	<style name="Light1.Button.Dark">
		<item name="android:textColor">#666666</item>
		<item name="android:shadowColor">#ffffff</item>
	</style>
	<style name="Light1.Button.Dark.Medium">
	  	<item name="android:textSize">16dp</item>
	</style>
	<style name="Light1.Button.Dark.Medium.Bold">
		<item name="android:textStyle">bold</item>
	</style>
</resources>

